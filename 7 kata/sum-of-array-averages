function sumAverage(arr) {
  var result = 0;
  // Your code here
  console.log(arr);
  arr.forEach(function(a){
    let sum = a.reduce(function(a,b){
      return a + b;
    })
    //console.log(avg);
    let avg = sum/a.length;
    console.log(avg);
    result += avg;
  })
  return Math.floor(result);
}
